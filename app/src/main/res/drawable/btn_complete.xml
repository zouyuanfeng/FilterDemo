<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ffdf580e" />
            <stroke android:width="1dp" android:color="#ffd8550d" />

        </shape>
    </item>

    <item android:state_enabled="false">
        <shape>
            <solid android:color="#a5ff6510" />
            <stroke android:width="1dp" android:color="#e7ff6510" />
        </shape>
    </item>


    <item>
        <shape>
            <solid android:color="#ffff6510" />
            <stroke android:width="1dp" android:color="#fff8620f" />
        </shape>
    </item>



</selector>