<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ffdf9a00" />
            <stroke android:width="1dp" android:color="#ffd89500" />

        </shape>
    </item>

    <item android:state_enabled="false">
        <shape>
            <solid android:color="#a5ffb000" />
            <stroke android:width="1dp" android:color="#e7ffb000" />
        </shape>
    </item>


    <item>
        <shape>
            <solid android:color="#ffffb000" />
            <stroke android:width="1dp" android:color="#fff8ab00" />
        </shape>
    </item>

</selector>